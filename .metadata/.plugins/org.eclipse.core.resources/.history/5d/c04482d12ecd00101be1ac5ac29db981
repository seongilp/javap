public class EmployeeTest {
	public static void main(String[] args) {
		Emplyee hong = new Employee();
		hong.name = "홍길동";
		hong.age = 20;
		System.out.println("한 사원의 이름은  " + hong.name + "이고, 나이는 " + hong.age + "입니다");
	}
}

class Employee {
	String 	name;
	int 	number;
	int 	age;
	String 	title;
	String 	dept;
	String 	grade;
}
