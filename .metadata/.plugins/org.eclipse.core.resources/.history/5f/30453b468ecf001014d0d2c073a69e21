		Vector v = new Vector();
		v.add(10);
		v.add(3.14);
		v.add("Java");
		Employee e1 = new Employee(100, "¿Ã¿⁄πŸ", 89000.99);
		v.add(e1);
	}
}

class Employee {
	int id;
	String name;
	double salary;
	public Employee(int id, String name, double salary) {
		this.id = id;
		this.name = name;
		this.salary = salary;
	}
}