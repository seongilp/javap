
class Adding {
	int add(int i, int j) {
		return i+j;
	}
	double add(double i, double j) {
		return i+j;
	}
	String add(String i, String j) {
		return i+j;
	}
}

public class OverloadingTest {
	public static void main(String[] args) {
		Adding a = new Adding();
		int result1 = a.add(1,2);
		System.out.println(result1);
		a.add(3.14, 2.2);
		System.out.println(a);
		a.add("java","jsp");
		System.out.println(a);
	}
}
