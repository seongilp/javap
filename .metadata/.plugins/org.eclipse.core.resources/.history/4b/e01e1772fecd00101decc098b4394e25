class Sam {

}

class Expr {
	void add(int i) {
		System.out.println(i++);
	}
	void add(Sam s) {
		System.out.println(s.a++);
	}
}
public class ExprTest {
	public static void main(String[] args) {
		Expr e = new Expr();
		int j = 10;
		e.add(j);
		System.out.println("e.add(j) 호출 후의  j값 " + j);
		
	}
}
